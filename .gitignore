# Default ignored files
/shelf/
/workspace.xml
# Editor-based HTTP Client requests
/httpRequests/
# Datasource local storage ignored files
/dataSources/
/dataSources.local.xml
# Terraform project directory
gcp-terraform-project/.terraform/*

# .tfstate files
gcp-terraform-project/*.tfstate
gcp-terraform-project/*.tfstate.*

# Crash log files
gcp-terraform-project/crash.log
gcp-terraform-project/crash.*.log

# Exclude all .tfvars files, which are likely to contain sensitive data
gcp-terraform-project/*.tfvars
gcp-terraform-project/*.tfvars.json

# Ignore override files as they are usually used for local dev
gcp-terraform-project/override.tf
gcp-terraform-project/override.tf.json
gcp-terraform-project/*_override.tf
gcp-terraform-project/*_override.tf.json

# Ignore CLI configuration files
gcp-terraform-project/.terraformrc
gcp-terraform-project/terraform.rc

# Ignore provider lock file
gcp-terraform-project/.terraform.lock.hcl

# GCP credentials
gcp-terraform-project/credentials.json
gcp-terraform-project/*.json

# IDE and editor files
.idea/
.vscode/
*.swp
*.swo

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db